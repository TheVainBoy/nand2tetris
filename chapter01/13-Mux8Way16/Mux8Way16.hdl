//  TheVainBoy

CHIP Mux8Way16 {
    IN a[16], b[16], c[16], d[16],
       e[16], f[16], g[16], h[16],
       sel[3];
    OUT out[16];

    PARTS:
    Mux4Way16(a=a, b=b, c=c, d=d, sel=sel[0..1], out=peter);
    Mux4Way16(a=e, b=f, c=g, d=h, sel=sel[0..1], out=andre);
    Mux16(a=peter, b=andre, sel=sel[2], out=out);
}
